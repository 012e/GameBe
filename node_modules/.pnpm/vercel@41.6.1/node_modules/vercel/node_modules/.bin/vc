#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tuan/code/cs106/ataxx/backend/node_modules/.pnpm/vercel@41.6.1/node_modules/vercel/dist/node_modules:/home/tuan/code/cs106/ataxx/backend/node_modules/.pnpm/vercel@41.6.1/node_modules/vercel/node_modules:/home/tuan/code/cs106/ataxx/backend/node_modules/.pnpm/vercel@41.6.1/node_modules:/home/tuan/code/cs106/ataxx/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tuan/code/cs106/ataxx/backend/node_modules/.pnpm/vercel@41.6.1/node_modules/vercel/dist/node_modules:/home/tuan/code/cs106/ataxx/backend/node_modules/.pnpm/vercel@41.6.1/node_modules/vercel/node_modules:/home/tuan/code/cs106/ataxx/backend/node_modules/.pnpm/vercel@41.6.1/node_modules:/home/tuan/code/cs106/ataxx/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/vc.js" "$@"
else
  exec node  "$basedir/../../dist/vc.js" "$@"
fi
